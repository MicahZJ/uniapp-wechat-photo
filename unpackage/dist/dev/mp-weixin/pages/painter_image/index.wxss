#mine-wrapper {
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}
#mine-wrapper .painter-box {
  width: 375px;
  height: 375px;
  position: relative;
  overflow: hidden;
}
#mine-wrapper .painter-box .background-css {
  width: 375px;
  height: 375px;
}
#mine-wrapper .painter-box .icon-css {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  width: 80px;
  height: 80px;
}
#mine-wrapper .preview-box {
  position: fixed;
  width: 100vw;
  height: 100vh;
  z-index: 2;
  background: #fff;
}
#mine-wrapper .preview-box .title {
  display: inline-block;
  width: 100%;
  text-align: center;
  font-size: 20px;
}
#mine-wrapper .preview-box .button-box {
  margin-top: 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 5%;
  box-sizing: border-box;
}
#mine-wrapper .preview-box .button-box .button-cancel {
  width: 45%;
}
#mine-wrapper .preview-box .button-box .button-confirm {
  width: 45%;
  color: #fff;
  background: #3684fe;
}
#mine-wrapper .preview-box .tt {
  overflow-x: auto;
}
#mine-wrapper .preview-box .button-play {
  margin-top: 20px;
  width: 90%;
  color: #fff;
  background: #3684fe;
}
#mine-wrapper .button-preview {
  width: 90%;
  margin-top: 10px;
}
#mine-wrapper .icon-box {
  margin-top: 10px;
  width: 100vw;
  height: auto;
  padding: 10px 20px;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border: 1px solid rgba(0,0,0,0.3);
  border-radius: 10px;
}
#mine-wrapper .icon-box .icon-css {
  width: 70px;
  height: 70px;
  border-radius: 10px;
  border: 1px dashed rgba(0,0,0,0.5);
  margin: 0 5px;
}

